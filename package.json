{
  "name": "wolfgang-feedback",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:build": "node ./build/index.js",
    "start:server": "cd server && nodemon index.js --exec babel-node --presets env",
    "start:client": "cd client && npm start",
    "start:dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "clean": "rm -rf build && mkdir build",
    "clean:win": "rd /s /q build && mkdir build",
    "build:dist": "babel -d ./build ./server",
    "build:src": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build:dist",
    "start": "npm run build && node ./build/index.js",
    "pipeline": "npm run clean:win && npm run build:src",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "keywords": [],
  "author": "Fionn Casey",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-validator": "^5.3.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.7",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.8",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.3"
  }
}
